			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"foreground": "#79B8FF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#79B8FF"
			}
		},
		{
			"scope": [
